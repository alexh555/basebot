	<robot name="panda_arm_net">
		<model dir="../../urdf" path="panda_arm_net.urdf" name="panda_arm_net" />
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	</robot>

	<static_object name="Base">
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
